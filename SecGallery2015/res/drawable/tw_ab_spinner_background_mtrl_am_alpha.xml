<?xml version="1.0" encoding="utf-8"?>
<selector android:autoMirrored="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <nine-patch android:src="@drawable/tw_spinner_mtrl_am_alpha" android:tint="@color/spinner_arrow_color" />
    </item>
    <item android:state_pressed="true">
        <nine-patch android:src="@drawable/tw_spinner_mtrl_am_alpha" android:tint="@color/spinner_arrow_color" />
    </item>
    <item android:state_enabled="false">
        <nine-patch android:src="@drawable/tw_spinner_mtrl_am_alpha" android:tint="@color/spinner_arrow_color" android:alpha="0.4" />
    </item>
    <item>
        <nine-patch android:src="@drawable/tw_spinner_mtrl_am_alpha" android:tint="@color/spinner_arrow_color" />
    </item>
</selector>