<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DialpadKeyLettersStyle">
        <item name="android:textSize">@dimen/dialpad_key_letters_text_size</item>
        <item name="android:textColor">@color/dialpad_secondary_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/dialpad_key_letters_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dialpad_key_letters_text_margin_top</item>
        <item name="android:importantForAccessibility">no</item>
    </style>
    <style name="DialpadKeyButtonStyle">
        <item name="android:background">@drawable/call_dial_btn_bg</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:clipChildren">false</item>
        <item name="android:layout_width">107.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:minHeight">@dimen/dialpad_key_height</item>
        <item name="android:soundEffectsEnabled">false</item>
    </style>
    <style name="DialpadKeyLettersStyleZero">
        <item name="android:textSize">@dimen/dialpad_key_letters_size_zero</item>
        <item name="android:textColor">@color/dialpad_secondary_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/dialpad_key_letters_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dialpad_key_letters_zero_margin_top</item>
        <item name="android:importantForAccessibility">no</item>
    </style>
    <style name="SecEmergencyContactItemStyle">
        <item name="android:textSize">@dimen/emergency_zero_contact_item_textSize</item>
        <item name="android:textColor">@color/dialpad_edittext_text_color</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="DialpadKeyButtonContainerStyle">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">107.0dip</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
</resources>
