<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="2.0dip" android:color="#ff00a0ce" />
                </shape>
            </item>
            <item android:drawable="@drawable/call_dial_call_btn_bg_inset" />
        </layer-list>
    </item>
    <item>
        <ripple android:color="#ff66c7ff">
            <item android:state_pressed="false" android:drawable="@drawable/call_dial_call_btn_bg_inset" />
        </ripple>
    </item>
</selector>