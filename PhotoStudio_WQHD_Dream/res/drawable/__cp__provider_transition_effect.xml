<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:enterFadeDuration="@integer/__cp__pressed_fade_in_time" android:exitFadeDuration="@integer/__cp__pressed_fade_out_time">
        <shape android:shape="oval">
            <solid android:color="#4dffffff" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/__cp__bluetooth_keyboard_rect_width" android:color="#ff0000ff" />
        </shape>
    </item>
</selector>